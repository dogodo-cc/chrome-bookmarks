{
  "manifest_version": 3,
  "name": "Better Bookmarks",
  "version": "1.0",
  "description": "一个更好用的书签管理器",
  "permissions": [
    "bookmarks",
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "chrome_url_overrides": {
    "bookmarks": "dist/bookmarks.html"
  }
}